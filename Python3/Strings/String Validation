if __name__ == '__main__':
    s = input()
    
    result = False
    for i in range(len(s)):
        if s[i].isalnum():
            result = True
            break
    print(result)

    result = False
    for i in range(len(s)):
        if s[i].isalpha():
            result = True
            break
    print(result)

    result = False
    for i in range(len(s)):
        if s[i].isdigit():
            result = True
            break
    print(result)

    result = False
    for i in range(len(s)):
        if s[i].islower():
            result = True
            break
    print(result)

    result = False
    for i in range(len(s)):
        if s[i].isupper():
            result = True
            break
    print(result)
